        "esg_terms = {\n",
        "    \"Environmental\": [\"emission\", \"pollution\", \"waste\", \"carbon\", \"climate\", \"deforestation\"],\n",
        "    \"Social\": [\"labor\", \"human rights\", \"discrimination\", \"diversity\", \"safety\", \"community\"],\n",
        "    \"Governance\": [\"fine\", \"lawsuit\", \"corruption\", \"fraud\", \"regulation\", \"penalty\"]\n",
        "}"
      ],
      "metadata": {
        "id": "UajIDRGUjRpN"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def extract_text_from_pdf(pdf_path):\n",
        "    text = \"\"\n",
        "    with pdfplumber.open(pdf_path) as pdf:\n",
        "        for page in pdf.pages:\n",
        "            page_text = page.extract_text()\n",
        "            if page_text:  # some pages may be empty\n",
        "                text += page_text + \" \"\n",
        "    return text.lower()"
      ],
      "metadata": {
        "id": "GvvXajGMjcrm"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def analyze_report(company, year, pdf_path):\n",
        "    text = extract_text_from_pdf(pdf_path)\n",
        "\n",
        "    records = []\n",
        "    total_score = 0\n",
        "    for category, keywords in esg_terms.items():\n",
        "        for word in keywords:\n",
        "            count = text.count(word)\n",
        "            if count > 0:\n",
        "                # Sentiment score for the whole report (basic check)\n",
        "                sentiment = TextBlob(text).sentiment.polarity\n",
        "                total_score += count\n",
        "                records.append([company, year, word, category, count, sentiment])\n",
        "\n",
        "    return records, total_score"
      ],
      "metadata": {
        "id": "-mRjKnQdjzai"
      },
      "execution_count": 6,
      "outputs": []
    },
